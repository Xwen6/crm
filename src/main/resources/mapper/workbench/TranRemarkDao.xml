<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wyu.xwen.workbench.dao.TranRemarkDao">

    <select id="getRemarkCount" resultType="int">
        select  count(*) from tbl_tran_remark where tranId = #{tranId}
    </select>

    <delete id="deleteTranRemarkByTId">
        delete from tbl_tran_remark where tranId = #{tranId}
    </delete>

</mapper>
